{
  "name": "thoughtfulai",
  "type": "module",
  "exports": {
    ".": "./sorter.mjs"
  },
  "version": "1.0.0",
  "description": "Thoughtful AI test",
  "main": "sorter.js",
  "scripts": {
    "test": "node --test",
    "fixcoverage": "standard --fix && c8 --reporter=text -- node --test",
    "coverage": "c8 --reporter=text -- node --test"
  },
  "c8": {
    "reporter": [
      "text",
      "cobertura",
      "lcov"
    ],
    "reporter-dir": "coverage",
    "all": true,
    "check-coverage": true,
    "clean": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "include": [
      "**/*.mjs"
    ],
    "exclude": [
      "test/*"
    ],
    "watermarks": {
      "lines": [
        70,
        80
      ],
      "functions": [
        70,
        80
      ],
      "statements": [
        70,
        80
      ],
      "branches": [
        70,
        80
      ]
    }
  },
  "author": "Trentin Bishop",
  "dependencies": {  },
  "devDependencies": {
    "c8": "10.1.3",
    "standard": "17.1.2"
  }
}